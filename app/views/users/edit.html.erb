<%= form_for @user, url: profile_edit_path, method: :put do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.submit "Submit Changes" %>
<% end %>
